<div class='jumbotron' id='landing'>
  <div class='splash-center'>
    <img src='http://res.cloudinary.com/dwhvrflyu/image/upload/v1439775160/grounded_logo_blue_umdzai.png'
         class='brand'>

    <div class='splash'>
      <div class='splash-inner'>
        <%= link_to "Find a Home", '/#listings', :class => 'btn btn-default btn-lg splash' %>
        <%= link_to "Why Grounded?", '/#slider', :class => 'btn btn-default btn-lg splash' %>
        <%= link_to "Invest in Properties", '/home', :class => 'btn btn-default btn-lg splash' %>
        <br>
        <button type="button" class="btn btn-primary btn-lg splash"
