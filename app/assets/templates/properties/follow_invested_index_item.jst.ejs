<div class="property_address panel-heading">
  <h5 id="street_address">
    <%= property.escape('street_number') + ' ' + property.escape('street')%>
  </h5>
  <h5 id="city_state_zip">
    <%= property.escape('city') + ", " + property.escape('state') + "  " + property.escape('zip') %>
  </h5>
</div>

<div class='panel-body'>
  <div class='progress'>
    <div class="progress-bar" role="progressbar"
                              aria-valuenow="<%= property.escape('investors') %>"
                              aria-valuemin="0" aria-valuemax="33"
                              style="min-width: 10%; width: <%= Math.round(property.escape('investors')/33 * 100)%>% ;">
      <%= Math.round(property.escape('investors')/33 * 100)%>%
    </div>
  </div>
</div>
